{
  "multipart": [
    {
      "apply": {
        "model": "hempfarmer:block/powerbattery_off"
      }
    },
    {
      "when": {
        "powered": "false"
      },
      "apply": {
        "model": "hempfarmer:block/powerbattery_off"
      }
    },
    {
      "when": {
        "powered": "true"
      },
      "apply": {
        "model": "hempfarmer:block/powerbattery_on"
      }
    }
  ]
}
